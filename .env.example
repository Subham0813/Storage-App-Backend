# Example environment variables for local development
# Copy this file to .env and fill in the values. Do NOT commit real secrets.

# Database
MONGO_URI=

# Session / JWT
SESSION_SECRET=

# Google OAuth (used for sign-in and Drive import)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=

# GitHub OAuth
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
GITHUB_REDIRECT_URI=

# Optional: Sentry/Monitoring, SMTP etc.
SENTRY_DSN=
SMTP_URL=

# Storage
UPLOADS_PATH=uploads
